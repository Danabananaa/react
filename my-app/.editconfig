[*.html]
ij_html_continuation_indent_size = 2
ij_html_align_attributes = true
ij_html_new_line_before_first_attribute = when_multiline
ij_html_new_line_after_last_attribute = when_multiline
ij_html_align_text = true
ij_html_attribute_wrap = on_every_item
ij_html_keep_blank_lines = 0
ij_html_keep_indents_on_empty_lines = false
ij_html_keep_whitespaces = false
ij_html_keep_line_breaks = false
ij_html_keep_line_breaks_in_text = true
ij_html_quote_style = double
ij_html_space_inside_empty_tag = false
ij_html_space_around_equality_in_attribute = false
ij_html_text_wrap = off
ij_html_uniform_ident = true


[{*.js,*.jsx,*.ts,*.tsx}]
indent_size = 2
indent_style = space
trim_trailing_whitespace = true
ij_any_line_comment_add_space = true
charset = utf-8
ij_any_align_multiline_parameters = true
ij_any_align_group_field_declarations = true
ij_any_align_multiline_array_initializer_expression = true
ij_any_align_multiline_chained_methods = true
ij_continuation_indent_size = 2
ij_javascript_array_initializer_new_line_after_left_brace = true
ij_javascript_array_initializer_right_brace_on_new_line = true
ij_javascript_align_multiline_parameters = true
ij_javascript_align_multiline_ternary_operation = true
ij_javascript_align_object_properties = 1
ij_javascript_align_imports = true
ij_javascript_force_quote_style = true
ij_javascript_align_multiline_extends_list = true
ij_any_assignment_wrap = on_every_item
ij_javascript_blank_lines_after_imports = 1
ij_javascript_chained_call_dot_on_new_line = true
ij_javascript_force_semicolon_style = true
ij_javascript_if_brace_force = always
ij_javascript_keep_indents_on_empty_lines = false
ij_javascript_spaces_around_additive_operators = true
ij_vue_interpolation_wrap = on_every_item
ij_vue_interpolation_new_line_after_start_delimiter = true
ij_vue_interpolation_new_line_before_end_delimiter = true
ij_vue_keep_indents_on_empty_lines = false
ij_vue_spaces_within_interpolation_expressions = true
ij_typescript_align_imports = true
ij_typescript_align_object_properties = 1
ij_typescript_enforce_trailing_comma = whenmultiline
ij_typescript_align_multiline_parameters = false
